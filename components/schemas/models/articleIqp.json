{
    "ArticleIQPSListResponse": {
        "type": "object",
        "properties": {
            "current_page": {
                "type": "integer",
                "description": "Numéro de la page actuelle."
            },
            "data": {
                "type": "array",
                "items": {
                    "$ref": "#articleIqp.json/ArticleIQPS"
                }
            },
            "first_page_url": {
                "type": "string",
                "description": "URL de la première page de résultats."
            },
            "from": {
                "type": "integer",
                "description": "Indice du premier élément dans la liste actuelle."
            },
            "last_page": {
                "type": "integer",
                "description": "Numéro de la dernière page."
            },
            "last_page_url": {
                "type": "string",
                "description": "URL de la dernière page de résultats."
            },
            "links": {
                "type": "array",
                "items": {
                    "$ref": "#articleIqp.json/PageLink"
                }
            },
            "next_page_url": {
                "type": "string",
                "description": "URL de la page suivante."
            },
            "path": {
                "type": "string",
                "description": "Chemin de base de l'API."
            },
            "per_page": {
                "type": "integer",
                "description": "Nombre d'éléments par page."
            },
            "prev_page_url": {
                "type": "string",
                "description": "URL de la page précédente."
            },
            "to": {
                "type": "integer",
                "description": "Indice du dernier élément dans la liste actuelle."
            },
            "total": {
                "type": "integer",
                "description": "Nombre total d'éléments."
            }
        }
    },
    "ArticleIQPS": {
        "type": "object",
        "properties": {
            "id": {
                "type": "string",
                "description": "Identifiant de l'article IQPS."
            },
            "name": {
                "type": "string",
                "description": "Nom de l'article IQPS."
            },
            "norme": {
                "type": "string",
                "description": "Norme de l'article IQPS."
            },
            "unite_mesure_id": {
                "type": "string",
                "description": "Identifiant de l'unité de mesure."
            },
            "article_id": {
                "type": "string",
                "description": "Identifiant de l'article."
            },
            "iqp_id": {
                "type": "string",
                "description": "Identifiant de l'indice de qualité perçu."
            },
            "created_at": {
                "type": "string",
                "format": "date-time",
                "description": "Date et heure de création de l'article IQPS."
            },
            "quantity_treated": {
                "type": "string",
                "description": "Quantité traitée de l'article IQPS."
            }
        }
    },
    "PageLink": {
        "type": "object",
        "properties": {
            "url": {
                "type": "string",
                "description": "URL de la page."
            },
            "label": {
                "type": "string",
                "description": "Étiquette de la page."
            },
            "active": {
                "type": "boolean",
                "description": "Indique si la page est active."
            }
        }
    }
}