{
    "PlanComptable": {
        "type": "object",
        "required": [
            "id",
            "name"
        ],
        "properties": {
            "id": {
                "$ref": "../identifier.json#/Identifier",
                "description": "Unique identifier for the plan comptable"
            },
            "name": {
                "type": "string",
                "default": "Tresorie passif",
                "unique": true,
                "description": "Nom du plan comptable"
            },
            "created_at": {
                "$ref": "../date_format.json#/DateFormat",
                "description": "Date and time when the plan comptable was created (YYYY-MM-DD HH:MM:SS)"
            }
        }
    }
}