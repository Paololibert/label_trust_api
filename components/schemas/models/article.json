{
    "ArticleListResponse": {
        "type": "object",
        "properties": {
            "current_page": {
                "type": "integer",
                "description": "Numéro de la page actuelle."
            },
            "data": {
                "type": "array",
                "items": {
                    "$ref": "#/components/schemas/ArticleResponse"
                },
                "description": "Liste des articles récupérés."
            },
            "first_page_url": {
                "type": "string",
                "description": "URL de la première page."
            },
            "from": {
                "type": "integer",
                "description": "Index du premier élément de la page actuelle."
            },
            "last_page": {
                "type": "integer",
                "description": "Numéro de la dernière page."
            },
            "last_page_url": {
                "type": "string",
                "description": "URL de la dernière page."
            },
            "links": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "url": {
                            "type": "string",
                            "description": "URL du lien."
                        },
                        "label": {
                            "type": "string",
                            "description": "Libellé du lien."
                        },
                        "active": {
                            "type": "boolean",
                            "description": "Indique si le lien est actif ou non."
                        }
                    }
                },
                "description": "Liens de pagination."
            },
            "next_page_url": {
                "type": "string",
                "description": "URL de la page suivante."
            },
            "path": {
                "type": "string",
                "description": "Chemin de la requête."
            },
            "per_page": {
                "type": "integer",
                "description": "Nombre d'articles par page."
            },
            "prev_page_url": {
                "type": "string",
                "description": "URL de la page précédente."
            },
            "to": {
                "type": "integer",
                "description": "Index du dernier élément de la page actuelle."
            },
            "total": {
                "type": "integer",
                "description": "Nombre total d'articles."
            }
        }
    },
    "ArticleResponse": {
        "type": "object",
        "properties": {
            "id": {
                "type": "string",
                "description": "ID de l'article."
            },
            "name": {
                "type": "string",
                "description": "Nom de l'article."
            },
            "created_at": {
                "type": "string",
                "format": "date-time",
                "description": "Date et heure de création de l'article."
            }
        }
    }
}