{
    "IQPListResponse": {
        "type": "object",
        "properties": {
            "status": {
                "type": "boolean",
                "description": "Indique le statut de la requête.",
                "default": true,
                "example": true
            },
            "message": {
                "type": "string",
                "description": "Informations supplémentaires sur la réponse.",
                "default": null,
                "example": null
            },
            "data": {
                "type": "object",
                "properties": {
                    "current_page": {
                        "type": "integer",
                        "description": "Le numéro de la page actuelle."
                    },
                    "data": {
                        "type": "array",
                        "items": {
                            "$ref": "#IQP"
                        },
                        "description": "Les données des IQPs."
                    },
                    "first_page_url": {
                        "type": "string",
                        "description": "L'URL de la première page."
                    },
                    "from": {
                        "type": "integer",
                        "description": "L'indice du premier élément dans les résultats."
                    },
                    "last_page": {
                        "type": "integer",
                        "description": "Le numéro de la dernière page."
                    },
                    "last_page_url": {
                        "type": "string",
                        "description": "L'URL de la dernière page."
                    },
                    "links": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "url": {
                                    "type": "string",
                                    "description": "L'URL de la page liée."
                                },
                                "label": {
                                    "type": "string",
                                    "description": "Le libellé de la page liée."
                                },
                                "active": {
                                    "type": "boolean",
                                    "description": "Indique si la page liée est active."
                                }
                            }
                        },
                        "description": "Les liens vers d'autres pages de résultats."
                    },
                    "next_page_url": {
                        "type": "string",
                        "description": "L'URL de la page suivante."
                    },
                    "path": {
                        "type": "string",
                        "description": "Le chemin de l'URL."
                    },
                    "per_page": {
                        "type": "integer",
                        "description": "Le nombre d'éléments par page."
                    },
                    "prev_page_url": {
                        "type": "string",
                        "description": "L'URL de la page précédente."
                    },
                    "to": {
                        "type": "integer",
                        "description": "L'indice du dernier élément dans les résultats."
                    },
                    "total": {
                        "type": "integer",
                        "description": "Le nombre total d'éléments."
                    }
                }
            },
            "status_code": {
                "type": "integer",
                "description": "Le code d'état HTTP de la réponse.",
                "default": 200,
                "example": 200
            }
        }
    },
    "IQP": {
        "type": "object",
        "properties": {
            "id": {
                "type": "string",
                "description": "L'identifiant de l'IQP."
            },
            "name": {
                "type": "string",
                "description": "Le nom de l'IQP."
            },
            "type_of_iqp": {
                "type": "string",
                "description": "Le type de l'IQP."
            },
            "created_at": {
                "type": "string",
                "description": "La date de création de l'IQP au format ISO 8601."
            },
            "iqp_type": {
                "type": "string",
                "description": "Le type de l'IQP."
            }
        }
    }
}