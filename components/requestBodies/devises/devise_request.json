{
    "DeviseRequest": {
        "type": "object",
        "required": [
            "name",
            "symbol"
        ],
        "properties": {
            "name": {
                "type": "string",
                "nullable": false,
                "example": "{{$randomCurrencyName}}",
                "default": "Franc CFA",
                "description": "Name of the devise"
            },
            "symbol": {
                "type": "string",
                "nullable": false,
                "example": "{{$randomCurrencySymbol}}",
                "default": "xof",
                "description": "Symbol de la devise"
            }
        }
    }
}