{
    "openapi": "3.0.0",
    "info": {
        "title": "Documentation API Partenaires",
        "version": "1.0.0",
        "description": "Documentation de l'API des partenaires."
    },
    "servers": [
        {
            "url": "http://127.0.0.1:8000/api"
        }
    ],
    "paths": {
        "/partners": {
            "get": {
                "summary": "Récupérer tous les partenaires",
                "responses": {
                    "200": {
                        "description": "Succès",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/PartnerListResponse"
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "summary": "Créer un nouveau partenaire",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/PartnerCreate"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Succès",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/PartnerResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/partners/{partnerId}": {
            "get": {
                "summary": "Récupérer un partenaire par ID",
                "parameters": [
                    {
                        "name": "partnerId",
                        "in": "path",
                        "required": true,
                        "description": "ID du partenaire",
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Succès",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/PartnerResponse"
                                }
                            }
                        }
                    }
                }
            },
            "put": {
                "summary": "Modifier un partenaire",
                "parameters": [
                    {
                        "name": "partnerId",
                        "in": "path",
                        "required": true,
                        "description": "ID du partenaire",
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/PartnerUpdate"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Succès",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/PartnerResponse"
                                }
                            }
                        }
                    }
                }
            },
            "delete": {
                "summary": "Supprimer un partenaire",
                "parameters": [
                    {
                        "name": "partnerId",
                        "in": "path",
                        "required": true,
                        "description": "ID du partenaire",
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "204": {
                        "description": "Succès"
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "PartnerListResponse": {
                "type": "object",
                "properties": {
                    "status": {
                        "type": "boolean",
                        "description": "Statut de la requête."
                    },
                    "message": {
                        "type": "string",
                        "nullable": true,
                        "description": "Message associé à la réponse."
                    },
                    "data": {
                        "type": "object",
                        "properties": {
                            "current_page": {
                                "type": "integer",
                                "description": "Numéro de la page actuelle."
                            },
                            "data": {
                                "type": "array",
                                "items": {
                                    "$ref": "#/components/schemas/PartnerResponse"
                                },
                                "description": "Liste des partenaires."
                            },
                            "first_page_url": {
                                "type": "string",
                                "description": "URL de la première page."
                            },
                            "from": {
                                "type": "integer",
                                "description": "Index du premier élément de la page actuelle."
                            },
                            "last_page": {
                                "type": "integer",
                                "description": "Numéro de la dernière page."
                            },
                            "last_page_url": {
                                "type": "string",
                                "description": "URL de la dernière page."
                            },
                            "links": {
                                "type": "array",
                                "items": {
                                    "type": "object",
                                    "properties": {
                                        "url": {
                                            "type": "string",
                                            "nullable": true,
                                            "description": "URL du lien."
                                        },
                                        "label": {
                                            "type": "string",
                                            "description": "Libellé du lien."
                                        },
                                        "active": {
                                            "type": "boolean",
                                            "description": "Indicateur d'activation du lien."
                                        }
                                    }
                                },
                                "description": "Liste des liens de pagination."
                            },
                            "next_page_url": {
                                "type": "string",
                                "nullable": true,
                                "description": "URL de la page suivante."
                            },
                            "path": {
                                "type": "string",
                                "description": "Chemin d'accès de la requête actuelle."
                            },
                            "per_page": {
                                "type": "integer",
                                "description": "Nombre d'éléments par page."
                            },
                            "prev_page_url": {
                                "type": "string",
                                "nullable": true,
                                "description": "URL de la page précédente."
                            },
                            "to": {
                                "type": "integer",
                                "description": "Index du dernier élément de la page actuelle."
                            },
                            "total": {
                                "type": "integer",
                                "description": "Nombre total d'éléments."
                            }
                        }
                    }
                }
            },
            "PartnerCreate": {
                "type": "object",
                "properties": {
                    "country": {
                        "type": "string",
                        "nullable": true,
                        "description": "Pays du partenaire."
                    },
                    "company": {
                        "type": "string",
                        "nullable": true,
                        "description": "Nom de la compagnie du partenaire."
                    },
                    "type_partner": {
                        "type": "string",
                        "description": "Type de partenaire (client ou fournisseur)."
                    },
                    "user": {
                        "$ref": "#/components/schemas/UserCreate"
                    }
                },
                "required": [
                    "type_partner",
                    "user"
                ]
            },
            "PartnerResponse": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string",
                        "description": "ID du partenaire."
                    },
                    "country": {
                        "type": "string",
                        "nullable": true,
                        "description": "Pays du partenaire."
                    },
                    "company": {
                        "type": "string",
                        "nullable": true,
                        "description": "Nom de la compagnie du partenaire."
                    },
                    "type_partner": {
                        "type": "string",
                        "description": "Type de partenaire (client ou fournisseur)."
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time",
                        "description": "Date de création du partenaire."
                    },
                    "suppliers": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "id": {
                                    "type": "string",
                                    "description": "ID du fournisseur."
                                },
                                "created_at": {
                                    "type": "string",
                                    "format": "date-time",
                                    "description": "Date de création du lien avec le fournisseur."
                                }
                            }
                        },
                        "description": "Liste des fournisseurs associés au partenaire."
                    },
                    "user": {
                        "type": "object",
                        "properties": {
                            "id": {
                                "type": "string",
                                "description": "ID de l'utilisateur associé au partenaire."
                            },
                            "username": {
                                "type": "string",
                                "description": "Nom d'utilisateur de l'utilisateur associé au partenaire."
                            },
                            "phone_number": {
                                "type": "string",
                                "nullable": true,
                                "description": "Numéro de téléphone de l'utilisateur associé au partenaire."
                            },
                            "address": {
                                "type": "string",
                                "nullable": true,
                                "description": "Adresse de l'utilisateur associé au partenaire."
                            },
                            "email": {
                                "type": "string",
                                "description": "Email de l'utilisateur associé au partenaire."
                            },
                            "created_at": {
                                "type": "string",
                                "format": "date-time",
                                "description": "Date de création de l'utilisateur associé au partenaire."
                            }
                        },
                        "description": "Détails de l'utilisateur associé au partenaire."
                    }
                }
            },
            "UserCreate": {
                "type": "object",
                "properties": {
                    "username": {
                        "type": "string",
                        "description": "Nom d'utilisateur."
                    },
                    "phone_number": {
                        "type": "string",
                        "nullable": true,
                        "description": "Numéro de téléphone."
                    },
                    "address": {
                        "type": "string",
                        "nullable": true,
                        "description": "Adresse."
                    },
                    "email": {
                        "type": "string",
                        "description": "Email."
                    }
                },
                "required": [
                    "username",
                    "email"
                ]
            },
            "PartnerUpdate": {
                "type": "object",
                "properties": {
                    "country": {
                        "type": "string",
                        "nullable": true,
                        "description": "Pays du partenaire."
                    },
                    "company": {
                        "type": "string",
                        "nullable": true,
                        "description": "Nom de la compagnie du partenaire."
                    },
                    "type_partner": {
                        "type": "string",
                        "description": "Type de partenaire (client ou fournisseur)."
                    }
                }
            }
        }
    }
}