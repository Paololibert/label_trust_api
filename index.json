        }
    },
    "servers": [
        {
            "url": "{{ip_address}}:{{port}}/api",
            "description": "Local development environment",
            "variables": {
                "ip_address": {
                    "default": "http://localhost",
                    "description": "Ip address"
                },
                "port": {
                    "enum": [
                        "80",
                        "8000",
                        "8001"
                    ],
                    "default": "8000"
                }
            }
        },
        {
            "url": "{{ip_address}}/api",
            "description": "Staging environment",
            "variables": {
                "ip_address": {
                    "default": "https://label-trust-api.ani-cashewpms.com",
                    "description": "Ip address"
                }
            }
        }
    ],
    "tags": [
